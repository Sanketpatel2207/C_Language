#include<stdio.h>
#include<math.h>

int main(){
    int n,i,sqr=0,prime=0;
    printf("Enter value of n:\n");
    scanf("%d",&n);    
    sqr=sqrt(n);
    for (i=2; i<=sqr; i++)
    {
     if (sqr%2==0)
        {
            prime=1;
        }
    else {
        prime=0;
        }
    }    
    if (prime)
    {
        printf("%d is a prime number",n);
    }
    
    else {
        printf("%d is not a prime number",n);
    }

    return 0;
}
